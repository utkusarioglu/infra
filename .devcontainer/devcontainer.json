{
  "name": "infra.nextjs-grpc.projects.utkusarioglu.com",
  "dockerComposeFile": [
    "../.docker/docker-compose.common.yml",
    "../.docker/docker-compose.dev.yml",
    "docker-compose.yml"
  ],
  "postCreateCommand": "scripts/post-create-command.sh",
  "service": "nextjs-grpc-infra",
  "workspaceFolder": "/utkusarioglu-com/projects/nextjs-grpc/infra",
  "customizations": {
    "vscode": {
      "extensions": [
        "golang.go",
        "hashicorp.terraform",
        "hashicorp.hcl",
        "ms-kubernetes-tools.vscode-kubernetes-tools",
        "amazonwebservices.aws-toolkit-vscode",
        "cschleiden.vscode-github-actions"
      ],
      "settings": {
        "workbench.colorCustomizations": {
          "activityBar.background": "#171244",
          "activityBar.foreground": "#ffffff"
        }
      }
    }
  }
}
